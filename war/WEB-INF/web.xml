<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.timeline.project.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>storeEvent</servlet-name>
    <servlet-class>com.timeline.project.server.StoreEvent</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>deleteEvent</servlet-name>
    <servlet-class>com.timeline.project.server.DeleteEventServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>getEvents</servlet-name>
  	<servlet-class>com.timeline.project.server.GetEvents</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/timeline/greet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>storeEvent</servlet-name>
    <url-pattern>/timeline/createEvent/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>getEvents</servlet-name>
  	<url-pattern>/timeline/getEvents</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>deleteEvent</servlet-name>
    <url-pattern>/timeline/deleteEvent</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
</web-app>